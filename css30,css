*/

/* 1. KEYFRAME FOR THE GENTLE TEASE (RESTORED) */
@keyframes gentle-tease {
  0%, 80% {
    transform: translateX(-10px) scale(1.1) rotate(0deg);
  }
  84% {
    transform: translateX(-10px) scale(1.1) rotate(-4deg);
  }
  88% {
    transform: translateX(-10px) scale(1.1) rotate(3deg);
  }
  92% {
    transform: translateX(-10px) scale(1.1) rotate(-1.5deg);
  }
  96% {
    transform: translateX(-10px) scale(1.1) rotate(0.8deg);
  }
  100% {
    transform: translateX(-10px) scale(1.1) rotate(0deg);
  }
}

/* 2. KEYFRAME FOR THE INFINITE BORDER GRADIENT SWEEP (UNCHANGED) */
@keyframes premium-tempt {
  to {
    background-position: 180% center;
  }
}


/* --- Button & Border Styling --- */

/* 1. Base button styles:
   - Restores the gentle tease animation and its base transform.
   - Adds a static, non-blinking neon glow.
*/
button.ant-btn:has(.anticon-star svg path[d*="zM"]) {
  position: relative;
  isolation: isolate;
  border: none !important;
  box-shadow: none !important;
  /* Restore the base transform for the tease animation */
  transform: translateX(-10px) scale(1.1);
  /* Apply the gentle tease animation */
  animation: gentle-tease 5.5s ease-in-out infinite;
}

/* 2. Animated gradient border:
   - Covers the button's black edge.
   - Continues its smooth sweep animation.
*/
button.ant-btn:has(.anticon-star svg path[d*="zM"])::before {
  content: "";
  position: absolute;
  z-index: -1;
  inset: -0.8px; /* Covers the button edge */
  border-radius: inherit;
  padding: 1.5px;
  background: linear-gradient(90deg, #ff007f, #ff69b4, #ff1493, #c71585, #8b008b, #9400d3, #ff007f);
  background-size: 180% 100%;
  -webkit-mask: linear-gradient(#fff 0 0) content-box, linear-gradient(#fff 0 0);
          mask: linear-gradient(#fff 0 0) content-box, linear-gradient(#fff 0 0);
  -webkit-mask-composite: xor;
          mask-composite: exclude;
  animation: premium-tempt 2.8s linear infinite;
}


/* --- Star Icon Styling --- */

/* 1. Style the SVG container for a clean, no-glow look. */
button.ant-btn:has(.anticon-star svg path[d*="zM"]) .anticon-star svg {
  animation: none;
  /* We ensure no filter is on the SVG container itself */
  filter: none;
}

/* 2. Style the SVG path for a solid, crisp white fill. */
